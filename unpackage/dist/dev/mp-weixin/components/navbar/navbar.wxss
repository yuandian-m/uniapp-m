.icon-search:before {
	content: "\e653";
}
.navbar .navbar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  background-color: #f07373;
}
.navbar .navbar-fixed .navbar-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  height: 45px;
  box-sizing: border-box;
}
.navbar .navbar-fixed .navbar-content .navbar-search {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 10px;
  width: 100%;
  height: 30px;
  border-radius: 30px;
  background-color: #fff;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search_icon {
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search_text {
  width: 100%;
  font-size: 14px;
  color: #999;
}
.navbar .navbar-fixed .navbar-content.search {
  padding-left: 0;
}
.navbar .navbar-fixed .navbar-content.search .navbar-content__search-icons {
  margin-left: 10px;
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content.search .navbar-search {
  border-radius: 5px;
}

